<div id="login">

  <h1>Sign in to start blogging!</h1>
    <form action="/users/login" method="post">
      <div>
        <input class="text-input" type="text" name="username" placeholder="Username">
      </div>
      <div>
        <input class="text-input" type="password" name="password" placeholder="Password">
      </div>
      <div>
        <input class="btn" type="submit" value="Login">
      </div>
    </form>
    <a id="create-account" href="/users/create_account">Don't have an account?  Sign up here</a>

</div>
